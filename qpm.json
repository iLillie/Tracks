{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Tracks",
    "id": "tracks",
    "version": "2.5.0",
    "url": "https://github.com/StackDoubleFlow/Tracks",
    "additionalData": {
      "overrideSoName": "libtracks.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1 --release"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "copyRelease": [
        "pwsh ./scripts/copy.ps1 --release"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "Tracks.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^7.4.2",
      "additionalData": {
        "extraFiles": []
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "4200.*",
      "additionalData": {
        "includeQmod": true
      }
    },
    {
      "id": "custom-json-data",
      "versionRange": "^0.25.0",
      "additionalData": {}
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.43",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.19.0",
      "additionalData": {
        "includeQmod": true
      }
    },
    {
      "id": "songcore",
      "versionRange": "^1.2.0",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "flamingo",
      "versionRange": "^1.1.2",
      "additionalData": {}
    }
  ]
}